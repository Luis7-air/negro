class NombreDeLaClase{
    constructor(atributo){
       this.propiedad=atributo
    }
    metodo(){
        console.log(this.propiedad)
    }
}
let intancia = new NombreDeLaClase("valor")
intancia.metodo()
let variable = intancia.propiedad

